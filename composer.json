{
    "name": "smokymountains/ean-client",
    "description": "Client for consuming the EAN Hotel API, based on Guzzle - forked for Guzzle 6.0",
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle-services": ">=6.0"
    },
    "require-dev": {
        "guzzlehttp/log-subscriber": "~1.0",
        "guzzlehttp/cache-subscriber": "~0.2@dev",
        "friendsofphp/php-cs-fixer": "^1.11",
        "phpunit/phpunit": "^5.7||^4.8"
    },
    "autoload": {
        "psr-4": {
            "Otg\\Ean\\": "src/",
            "Otg\\Ean\\Tests\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Ryan Hughes",
            "email": "rhughes@hotels.com.au"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT"
}
